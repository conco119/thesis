<div class="resize-default">
	<div class="col-md-3 col-sm-6 col-xs-12 col-df text-center">
	<div class="defaul_item">
		{foreach from=$menus item=data}
		{if $data.position eq '1'}
		<div class="box text-center {$data.size}">
			<a href="{$data.url}" title="{$data.name}">
				<div class="box-cont {$data.color}">
					<img src="{$data.image}">
					<p class="absolute">{$data.name}</p>
				</div>
			</a>
		</div>
		{/if}
		{/foreach}
	</div>
</div>

